{
  "name": "tauri-demo",
  "private": true,
  "version": "0.3.0",
  "engines": {
    "node": ">=16.17.0"
  },
  "scripts": {
    "dev": "vite",
    "debug:rs": "RUST_BACKTRACE=1 tauri dev",
    "build": "vite build",
    "build:tauri": "tauri build",
    "preview": "vite preview",
    "tauri": "tauri",
    "format": "prettier --cache --write \"**/*.{ts, vue}\"",
    "format:rs": "cargo fmt --manifest-path src-tauri/Cargo.toml",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx,.vue",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "check:all": "yarn lint && yarn format && yarn format:rs && yarn coverage"
  },
  "dependencies": {
    "@solid-primitives/media": "^2.0.4",
    "@solidjs/router": "^0.5.1",
    "@tanstack/solid-query": "^4.18.0",
    "@tauri-apps/api": "^1.2.0",
    "luxon": "^3.1.0",
    "solid-js": "^1.6.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.31.0",
    "@fontsource/fira-mono": "^4.5.10",
    "@fontsource/inter": "^4.5.14",
    "@fontsource/poppins": "^4.5.10",
    "@tailwindcss/typography": "^0.5.8",
    "@tauri-apps/cli": "^1.2.0",
    "@types/luxon": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitest/coverage-c8": "^0.25.3",
    "autoprefixer": "^10.4.13",
    "c8": "^7.12.0",
    "daisyui": "^2.42.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-solid": "^0.9.1",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-solid": "^2.4.0",
    "vitest": "^0.25.3"
  }
}
