{
  "name": "tauri-demo",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": ">=16.17.0"
  },
  "scripts": {
    "dev": "vite",
    "debug:rs": "RUST_BACKTRACE=1 tauri dev",
    "build": "vue-tsc --noEmit && vite build",
    "build:tauri": "tauri build",
    "preview": "vite preview",
    "tauri": "tauri",
    "format": "prettier --cache --write \"**/*.{ts, vue}\"",
    "format:rs": "cargo fmt --manifest-path src-tauri/Cargo.toml",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx,.vue",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "check:all": "yarn lint && yarn format && yarn format:rs && yarn coverage"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.2",
    "@vueuse/core": "^9.1.1",
    "@vueuse/motion": "^2.0.0-beta.12",
    "pinia": "^2.0.21",
    "vue": "^3.2.37",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.9",
    "@fontsource/inter": "^4.5.12",
    "@fontsource/poppins": "^4.5.9",
    "@tailwindcss/typography": "^0.5.4",
    "@tauri-apps/cli": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitest/coverage-c8": "^0.22.1",
    "autoprefixer": "^10.4.8",
    "c8": "^7.12.0",
    "daisyui": "^2.24.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^9.2.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.8.2",
    "vite": "^3.0.9",
    "vitest": "^0.22.1",
    "vue-tsc": "^0.40.2"
  }
}
