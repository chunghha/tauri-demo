{
  "name": "tauri-demo",
  "version": "0.4.1",
  "private": true,
  "engines": {
    "node": ">=18.18.2 <19.0.0",
    "pnpm": ">=8.10.5 <9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "debug:rs": "RUST_BACKTRACE=1 tauri dev",
    "build": "vite build",
    "build:tauri": "tauri build",
    "preview": "vite preview",
    "tauri": "tauri",
    "format": "prettier --cache --write \"**/*.{cjs,js,ts,tsx}\"",
    "format:rs": "cargo fmt --manifest-path src-tauri/Cargo.toml",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "check:all": "pnpm format && pnpm format:rs && pnpm lint:fix"
  },
  "dependencies": {
    "@solid-primitives/media": "^2.2.5",
    "@solidjs/router": "^0.9.1",
    "@tanstack/solid-query": "^5.12.1",
    "@tauri-apps/api": "^1.5.1",
    "luxon": "^3.4.4",
    "solid-js": "^1.8.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.3.4",
    "@fontsource/fira-mono": "^5.0.8",
    "@fontsource/nunito-sans": "^5.0.8",
    "@fontsource/tienne": "^5.0.8",
    "@tailwindcss/typography": "^0.5.10",
    "@tauri-apps/cli": "^1.5.7",
    "@types/luxon": "^3.3.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitest/coverage-v8": "^1.0.1",
    "autoprefixer": "^10.4.16",
    "daisyui": "^4.4.19",
    "eslint": "^8.55.0",
    "eslint-config-flat-gitignore": "^0.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-solid": "^0.13.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "tailwindcss": "^3.3.6",
    "typescript": "~5.3.2",
    "vite": "~5.0.5",
    "vite-plugin-solid": "^2.7.2",
    "vitest": "^1.0.1"
  }
}
