{
  "name": "tauri-demo",
  "private": true,
  "version": "0.3.0",
  "engines": {
    "node": ">=16.17.0"
  },
  "scripts": {
    "dev": "vite",
    "debug:rs": "RUST_BACKTRACE=1 tauri dev",
    "build": "vite build",
    "build:tauri": "tauri build",
    "preview": "vite preview",
    "tauri": "tauri",
    "format": "prettier --cache --write \"**/*.{ts, vue}\"",
    "format:rs": "cargo fmt --manifest-path src-tauri/Cargo.toml",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx,.vue",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "check:all": "yarn lint && yarn format && yarn format:rs && yarn coverage"
  },
  "dependencies": {
    "@solid-primitives/media": "^2.0.6",
    "@solidjs/router": "^0.7.0",
    "@tanstack/solid-query": "^4.24.4",
    "@tauri-apps/api": "^1.2.0",
    "luxon": "^3.2.0",
    "solid-js": "^1.6.10"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@fontsource/fira-mono": "^4.5.10",
    "@fontsource/inter": "^4.5.15",
    "@fontsource/poppins": "^4.5.10",
    "@tailwindcss/typography": "^0.5.9",
    "@tauri-apps/cli": "^1.2.3",
    "@types/luxon": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitest/coverage-c8": "^0.28.4",
    "autoprefixer": "^10.4.13",
    "c8": "^7.12.0",
    "daisyui": "^2.50.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-solid": "^0.9.4",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "tailwindcss": "^3.2.6",
    "typescript": "~4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-solid": "^2.5.0",
    "vitest": "^0.28.4"
  }
}
