{
  "name": "tauri-demo",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "debug:rs": "RUST_BACKTRACE=1 tauri dev",
    "build": "vue-tsc --noEmit && vite build",
    "build:tauri": "tauri build",
    "preview": "vite preview",
    "tauri": "tauri",
    "format": "prettier --cache --write \"**/*.{ts, vue}\"",
    "format:rs": "cargo fmt --manifest-path src-tauri/Cargo.toml",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx,.vue",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "check:all": "yarn lint && yarn format && yarn format:rs && yarn coverage"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.1",
    "@vueuse/core": "^8.7.5",
    "@vueuse/motion": "^2.0.0-beta.12",
    "pinia": "^2.0.14",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@fontsource/montserrat": "^4.5.11",
    "@fontsource/roboto": "^4.5.7",
    "@fontsource/space-mono": "^4.5.8",
    "@tailwindcss/typography": "^0.5.2",
    "@tauri-apps/cli": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.30.3",
    "@typescript-eslint/parser": "^5.30.3",
    "@vitejs/plugin-vue": "^2.3.3",
    "autoprefixer": "^10.4.7",
    "c8": "^7.11.3",
    "daisyui": "^2.17.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^9.1.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.4",
    "typescript": "~4.7.4",
    "vite": "^2.9.13",
    "vitest": "^0.16.0",
    "vue-tsc": "^0.38.2"
  }
}
