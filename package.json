{
  "name": "tauri-demo",
  "private": true,
  "version": "0.3.0",
  "engines": {
    "node": ">=16.17.0"
  },
  "scripts": {
    "dev": "vite",
    "debug:rs": "RUST_BACKTRACE=1 tauri dev",
    "build": "vite build",
    "build:tauri": "tauri build",
    "preview": "vite preview",
    "tauri": "tauri",
    "format": "prettier --cache --write \"**/*.{ts, vue}\"",
    "format:rs": "cargo fmt --manifest-path src-tauri/Cargo.toml",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx,.vue",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "check:all": "yarn lint && yarn format && yarn format:rs && yarn coverage"
  },
  "dependencies": {
    "@solid-primitives/media": "^2.1.4",
    "@solidjs/router": "^0.8.0",
    "@tanstack/solid-query": "^4.26.1",
    "@tauri-apps/api": "^1.2.0",
    "luxon": "^3.3.0",
    "solid-js": "^1.6.15"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.36.0",
    "@fontsource/fira-mono": "^4.5.10",
    "@fontsource/inter": "^4.5.15",
    "@fontsource/poppins": "^4.5.10",
    "@tailwindcss/typography": "^0.5.9",
    "@tauri-apps/cli": "^1.2.3",
    "@types/luxon": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitest/coverage-c8": "^0.29.3",
    "autoprefixer": "^10.4.14",
    "c8": "^7.13.0",
    "daisyui": "^2.51.4",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-solid": "^0.11.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "tailwindcss": "^3.2.7",
    "typescript": "~5.0.2",
    "vite": "~4.2.0",
    "vite-plugin-solid": "^2.6.1",
    "vitest": "^0.29.3"
  }
}
