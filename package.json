{
  "name": "tauri-demo",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:tauri": "tauri build",
    "preview": "vite preview",
    "tauri": "tauri",
    "format": "prettier --write \"**/*.{ts, vue}\"",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx,.vue",
