{
  "name": "tauri-demo",
  "private": true,
  "version": "0.4.0",
  "engines": {
    "node": ">=16.17.0"
  },
  "scripts": {
    "dev": "vite",
    "debug:rs": "RUST_BACKTRACE=1 tauri dev",
    "build": "vite build",
    "build:tauri": "tauri build",
    "preview": "vite preview",
    "tauri": "tauri",
    "format": "prettier --cache --write \"**/*.{ts, vue}\"",
    "format:rs": "cargo fmt --manifest-path src-tauri/Cargo.toml",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx,.vue",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "check:all": "yarn lint && yarn format && yarn format:rs && yarn coverage"
  },
  "dependencies": {
    "@solid-primitives/media": "^2.2.3",
    "@solidjs/router": "^0.8.2",
    "@tanstack/solid-query": "^4.29.11",
    "@tauri-apps/api": "^1.3.0",
    "luxon": "^3.3.0",
    "solid-js": "^1.7.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@fontsource/fira-mono": "^5.0.3",
    "@fontsource/inter": "^5.0.3",
    "@fontsource/poppins": "^5.0.3",
    "@tailwindcss/typography": "^0.5.9",
    "@tauri-apps/cli": "^1.3.1",
    "@types/luxon": "^3.3.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitest/coverage-v8": "^0.32.0",
    "autoprefixer": "^10.4.14",
    "c8": "^7.14.0",
    "daisyui": "^3.0.25",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-solid": "^0.12.1",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.1.3",
    "vite": "~4.3.9",
    "vite-plugin-solid": "^2.7.0",
    "vitest": "^0.32.0"
  }
}
