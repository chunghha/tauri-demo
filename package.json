{
  "name": "tauri-demo",
  "private": true,
  "version": "0.4.0",
  "engines": {
    "node": ">=16.17.0"
  },
  "scripts": {
    "dev": "vite",
    "debug:rs": "RUST_BACKTRACE=1 tauri dev",
    "build": "vite build",
    "build:tauri": "tauri build",
    "preview": "vite preview",
    "tauri": "tauri",
    "format": "prettier --cache --write \"**/*.{ts, vue}\"",
    "format:rs": "cargo fmt --manifest-path src-tauri/Cargo.toml",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx,.vue",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "check:all": "yarn lint && yarn format && yarn format:rs && yarn coverage"
  },
  "dependencies": {
    "@solid-primitives/media": "^2.2.5",
    "@solidjs/router": "^0.8.3",
    "@tanstack/solid-query": "^4.35.3",
    "@tauri-apps/api": "^1.4.0",
    "luxon": "^3.4.3",
    "solid-js": "^1.7.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.43.1",
    "@fontsource/fira-mono": "^5.0.8",
    "@fontsource/inter": "^5.0.8",
    "@fontsource/poppins": "^5.0.8",
    "@tailwindcss/typography": "^0.5.10",
    "@tauri-apps/cli": "^1.4.0",
    "@types/luxon": "^3.3.2",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@vitest/coverage-v8": "^0.34.5",
    "autoprefixer": "^10.4.16",
    "daisyui": "^3.7.7",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-solid": "^0.13.0",
    "postcss": "^8.4.30",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.2.2",
    "vite": "~4.4.9",
    "vite-plugin-solid": "^2.7.0",
    "vitest": "^0.34.5"
  }
}
